{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.3.3",
      "destination": "Content/css/",
      "files": [
        "css/bootstrap.css"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "Content/toastr/",
      "files": [
        "toastr.min.js",
        "toastr.css"
      ]
    },
    {
      "library": "jquery@3.7.1",
      "destination": "Content/jquery/",
      "files": [
        "jquery.min.js"
      ]
    }
  ]
}